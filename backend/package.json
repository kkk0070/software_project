{
  "name": "ecoride-backend",
  "version": "1.0.0",
  "description": "Backend API for EcoRide Admin Dashboard",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "init-db": "node src/config/initDatabase.js",
    "migrate": "node src/config/migrateDatabase.js",
    "migrate-2fa": "node src/config/migrate2FA.js",
    "migrate-encryption": "node src/config/migrateEncryption.js",
    "migrate-chat": "node src/config/migrateChat.js",
    "migrate-profile-photo": "node src/config/migrateProfilePhoto.js",
    "migrate-all": "node src/config/runAllMigrations.js",
    "fix-chat": "node src/config/checkAndFixChatTables.js",
    "seed-dummy": "node src/config/seedDummyData.js",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage"
  },
  "keywords": [
    "ecoride",
    "backend",
    "api",
    "postgresql"
  ],
  "author": "EcoRide Team",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.13",
    "pg": "^8.11.3",
    "socket.io": "^4.8.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.29.0",
    "@types/jest": "^30.0.0",
    "babel-jest": "^30.2.0",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.2.2"
  }
}
